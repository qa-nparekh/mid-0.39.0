{
  "name": "@sqaitech/mcp",
  "version": "0.5.0",
  "description": "Deprecated - Use @sqaitech/web-bridge-mcp, @sqaitech/android-mcp, or @sqaitech/ios-mcp",
  "bin": "dist/index.js",
  "files": ["dist"],
  "main": "./dist/server.js",
  "types": "./dist/server.d.ts",
  "exports": {
    ".": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    }
  },
  "scripts": {
    "build": "rslib build",
    "dev": "npm run build:watch",
    "build:watch": "rslib build --watch --no-clean",
    "mcp-playground": "npx @modelcontextprotocol/inspector node ./dist/index.js",
    "inspect": "node scripts/inspect.mjs"
  },
  "dependencies": {},
  "devDependencies": {
    "@sqaitech/android": "workspace:*",
    "@sqaitech/core": "workspace:*",
    "@sqaitech/report": "workspace:*",
    "@sqaitech/shared": "workspace:*",
    "@sqaitech/web": "workspace:*",
    "@modelcontextprotocol/inspector": "^0.16.3",
    "@modelcontextprotocol/sdk": "1.10.2",
    "@rslib/core": "^0.18.3",
    "@types/node": "^18.0.0",
    "typescript": "^5.8.3"
  },
  "license": "MIT"
}
